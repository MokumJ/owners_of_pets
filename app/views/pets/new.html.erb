<%= form_for [@pet.owner, @pet] do |f| %>
 <%= f.label :name %>
 <%= f.text_field :name %>

<%= f.label :age %>
<%= f.number_field :age %>

<%= f.label :img_url %>
<%= f.text_field :img_url %>

<%= f.label :kind %>
<%= f.text_field :kind %>

<%= f.label :owner_id %>
 <%= f.collection_select(:owner, Owner.all, :id, :name) %>

<%= f.submit "Create" %>
<% end %>
